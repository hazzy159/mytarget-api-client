{
    "name": "kradwhite/mytarget-api-client",
    "type": "library",
    "description": "MyTarget api client",
    "keywords": [
        "kradwhite",
        "myTarget",
		"api",
        "client"
    ],
    "homepage": "https://github.com/kradwhite/mytarget-api-client",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "kradwhite",
            "email": "kradwhite@mail.ru",
            "homepage": "https://github.com/kradwhite",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "guzzlehttp/guzzle": "^7.3",
        "kradwhite/mytarget-oauth2": "^0.2.4"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "kradwhite\\myTarget\\api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kradwhite\\myTarget\\api\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./codecept run unit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
